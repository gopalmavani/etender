            e.preventDefault();
            var myCookie = checkMode();
            if (myCookie == null) {
                document.cookie = "themeOption=1; domain=.gem.gov.in; path=/;";
                $('body').addClass('nightmode');
            } else {
                if(myCookie == 1) {
                    document.cookie = "themeOption=0; domain=.gem.gov.in; path=/;";
                    $('body').removeClass('nightmode');
                } else {
                    document.cookie = "themeOption=1; domain=.gem.gov.in; path=/;";
