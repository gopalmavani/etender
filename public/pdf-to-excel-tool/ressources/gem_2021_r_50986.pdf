<script>
    $(document).on("click", '.saveSpecs:visible', function (e) {

        e.preventDefault();
        var variantID = $('#currentVariant').val();
        var bid_type = $('#bid_type').val();
        var token = $('#token').val();
        var detailID = $(this).attr('data-biddetail');
        var itemID = $('#currentItem').val();
        var btype = $('.productMain[data-bdid=' + detailID + ']').data('ptype');//0=>product(goods), 1=> services                
        var ajaxexe = false;
        var is_boq_item = 0;
         var isblankss = 0;
        if (btype == 1)
        {
            var count = 0;
            var error_count = 0;
            var ttl_cnt = $('#ttl_cnt').val();
            $("[spec_type='core']").each(function () {
                if ($(this).val() == null || $(this).val() == '') {
                    error_count++;
                }
            });
            if (error_count == 0)
            {
                ajaxexe = true;
            } else
            {
                alert('Please select all specification parameters');
            }
            var minimum_rate = $('#minimum_rate').val();
            if(minimum_rate!='' && minimum_rate <= 0)
            {
                $('#minimum_rate').focus();
                alert('Please provide valid minimum rate');
                return false;
            }
        } else
        {
            const custom_param_availed = $("input[name='custom_params_required']:checked").val();
            var is_boq_item = $("#buyer_boq_file:visible").length;
            if(custom_param_availed === '1') {
                ajaxexe = validateCustomParamData() ? true : false;
                if(ajaxexe) {
                    const is_confirmed = $('#custom_param_undertaking').prop('checked');
                    if(!is_confirmed) {
                        alert('Please review and accept required undertaking');
                        ajaxexe = false;
                    }
                }
            } else if(is_boq_item === 1) {
                var file = $("#buyer_boq_file:visible");
                if((file)[0].files.length === 1) {
                    const file_size = (file)[0].files.item(0).size;
                    const file_name = (file)[0].files.item(0).name;
                    const file_ext = file_name.split('.')[file_name.split('.').length - 1].toLowerCase();

                    if(file_size <= 0 || file_size > 10485760 || $.inArray(file_ext, ['xls', 'xlsx']) == -1) {
                        file.val('');
                        alert('Please upload a valid excel file against BOQ document. Max size is 10MB.');
                        ajaxexe = false;
                    } else {
                        ajaxexe = true;
                    }
                } else {
                    file.val('');
                    alert('Please upload a valid excel file against BOQ document. Max size is 10MB.');
                    ajaxexe = false;
                }
            } else {
                ajaxexe = true;
            }
        }
        if (ajaxexe == true)
        {   
            var file_boq = '';
            if(is_boq_item === 1){
                var saveUrl = "https://bidplus.gem.gov.in/bidding/bidding/saveBoqSpecs/";
                var form_data = new FormData();
                var file_data = $('#buyer_boq_file').prop('files')[0];
                form_data.append('buyer_boq_file', file_data);
                form_data.append('bid_type', bid_type);
                form_data.append('token', token);
                form_data.append('detailID', detailID);
                form_data.append('itemID', itemID);
                form_data.append('btype', btype);
                form_data.append('variantID', variantID);
                $.ajax({
                    url: saveUrl,
                    async: false,
                    type: "POST",
                    data: form_data,
                    dataType: "text",
                    contentType: false,
                    processData: false,
                    success: function (data) {
                        if (data)
                        {
                            if (data == 1)
                            {
                                location.reload();
                            }

                        }
                    }
                });
                
            }else{
                var saveUrl = "https://bidplus.gem.gov.in/bidding/saveSpecs/";
                var form_data =  $('#form_att').serialize();
                $.ajax({
                    url: saveUrl,
                    async: false,
                    type: "POST",
                    data: $('#form_att').serialize() + "&bid_type=" + btype + "&variantID=" + variantID + "&itemID=" + itemID + "&detailID=" + detailID + "&token=" + token + "&csrf_bd_gem_nk=c2da279a598e7a15cb5f093a662de2da&minimum_rate="+minimum_rate+"&buyer_boq_file="+file_boq,
                    dataType: "html",
                    success: function (data) {
                        if (data)
                        {
                            if (data == 1)
                            {
                                location.reload();
                            }

                        }

                    }
                });
            }
            
            
        }

    });
